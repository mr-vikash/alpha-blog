<h1> Create An Article </h1>
<%= form_with scope: :article, url: articles_path, local: true do |form| %>
    <p>
        <%= form.label :title %></br>
        <%= form.text_field :title %>
    </p>
    <p>
        <%= form.label :description %></br>
        <%= form.text_field :description %>
    </p>
    <p>
       <%= form.submit %> 
    </p>
<% end %>